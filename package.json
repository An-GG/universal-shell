{
  "name": "await-shell",
  "type": "module",
  "version": "3.0.0",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "build": "tsm build",
    "test": "ava",
    "lint": "eslint src --fix"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsm"
    ]
  },
  "devDependencies": {
    "@tsmodule/tsm": "^8.1.0",
    "@types/cross-spawn": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "ava": "^4.0.1",
    "eslint": "^8.8.0",
    "typescript": "^4.5.5"
  }
}
