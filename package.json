{
  "name": "await-shell",
  "description": "Cross-platform async shell support for POSIX commands on Linux, MacOS, and Windows.",
  "type": "module",
  "version": "32.0.0",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "build": "tsmodule build",
    "dev": "tsmodule dev",
    "pretest": "tsmodule build -r",
    "test": "ava",
    "lint": "eslint src --fix",
    "prepare": "tsmodule build -r",
    "prepublishOnly": "tsmodule build"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^27.0.3"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsmodule"
    ]
  },
  "dependencies": {
    "chalk": "^5.0.1"
  }
}
