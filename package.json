{
  "name": "await-shell",
  "description": "Cross-platform async shell support for POSIX commands on Linux, MacOS, and Windows.",
  "type": "module",
  "version": "34.1.0",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "build": "tsmodule build -b",
    "dev": "tsmodule dev",
    "pretest": "tsmodule build -r",
    "test": "ava",
    "lint": "eslint src --fix",
    "prepare": "tsmodule build -r",
    "prepublishOnly": "tsmodule build -b"
  },
  "dependencies": {},
  "devDependencies": {
    "@tsmodule/tsmodule": "^31.2.0",
    "@types/node": "^17.0.21",
    "ava": "^4.1.0",
    "chalk": "^5.0.1"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsmodule"
    ]
  }
}
