{
  "name": "await-shell",
  "type": "module",
  "version": "7.2.0",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "build": "tsmodule build",
    "test": "ava",
    "lint": "eslint src --fix",
    "prepare": "tsmodule build --fast",
    "prepublishOnly": "tsmodule build"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^10.2.1"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsmodule"
    ]
  }
}
