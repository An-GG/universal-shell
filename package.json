{
  "name": "await-shell",
  "type": "module",
  "version": "29.0.0",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "build": "tsmodule build",
    "dev": "tsmodule dev",
    "pretest": "tsmodule build -f",
    "test": "ava",
    "lint": "eslint src --fix",
    "prepare": "tsmodule build --fast",
    "prepublishOnly": "tsmodule build"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^15.1.0"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsmodule"
    ]
  },
  "dependencies": {
    "chalk": "^5.0.0"
  }
}
